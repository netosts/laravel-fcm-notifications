{
    "name": "netosts/laravel-fcm-notifications",
    "description": "A robust and secure Firebase Cloud Messaging (FCM) notification system for Laravel applications",
    "keywords": [
        "laravel",
        "firebase",
        "fcm",
        "push-notifications",
        "notifications",
        "messaging",
        "mobile"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "netosts",
            "email": "netostt91@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFcmNotifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFcmNotifications\\Providers\\FcmNotificationsServiceProvider"
            ],
            "aliases": {
                "Fcm": "LaravelFcmNotifications\\Facades\\Fcm"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}